{
  "expo": {
    "owner": "wesley_marques",
    "name": "CorraAgil",
    "slug": "corraagil",
    "version": "1.1.5",
    "orientation": "portrait",
    "androidStatusBar": {
      "backgroundColor": "#12263A",
      "barStyle": "light-content",
      "translucent": false
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "ota": {
      "enabled": true
    },
    "icon": "./src/assets/icon.png",
    "scheme": "CorraAgil",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#12263A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.corraagil.corraagil",
      "buildNumber": "1.0.0"
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBXrJiBCFu4QGVe9_4jn5mF6wwzjtQKXac"
        }
      },
      "package": "com.corraagil.corraagil",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#12263A"
      }
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Este app usa sua localização para registrar corridas.",
          "locationWhenInUsePermission": "Este app usa sua localização para registrar corridas."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2899d8e4-7d0f-4200-8b1d-12f1823f0846"
      }
    }
  }
}